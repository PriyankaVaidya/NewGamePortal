{
  "homepage": "http://yoav-zibin.github.io/NewGamePortal",
  "name": "portal",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "konva": "^1.7.6",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-konva": "^1.6.6",
    "react-redux": "^5.0.7",
    "react-scripts-ts": "2.13.0",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6"
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "ci-test": "CI=true react-scripts-ts test --env=jsdom",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "predeploy": "npm run build",
    "deploy": "CI=true react-scripts-ts test --env=jsdom & gh-pages -d build"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run ci-test"
    }
  },
  "devDependencies": {
    "@types/firebase": "^3.2.1",
    "@types/jest": "^22.1.2",
    "@types/material-ui": "^0.20.5",
    "@types/node": "^4.2.23",
    "@types/react": "^16.0.36",
    "@types/react-dom": "^16.0.3",
    "@types/react-redux": "^5.0.15",
    "@types/redux-logger": "^3.0.5",
    "gh-pages": "^1.1.0",
    "husky": "^0.15.0-rc.8",
    "lint-staged": "^7.0.0",
    "material-ui": "^0.20.0",
    "prettier": "^1.10.2",
    "tslint": "^5.9.1",
    "typescript": "^2.7.1"
  }
}
